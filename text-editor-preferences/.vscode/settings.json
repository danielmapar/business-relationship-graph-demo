{
    "workbench.colorTheme": "Default Dark Modern",
    "files.autoSave": "afterDelay",
    "redhat.telemetry.enabled": false,
    "gradle.reuseTerminals": "all",
    "gradle.allowParallelRun": false,
    "gradle.nestedProjects": ["platform-api"],
    "gradle.autoDetect": "on",
    "java.jdt.ls.androidSupport.enabled": "off",
    "java.import.projectSelection": "manual",
    "java.project.sourcePaths": ["platform-api"],
    "java.compile.nullAnalysis.mode": "automatic",
    "quarkus.tools.alwaysShowWelcomePage": false,
    "java.configuration.updateBuildConfiguration": "interactive",
    "python.analysis.extraPaths": [
        "${workspaceFolder}",
        "./community-repositories/workspace-worker-libraries/python/library"
    ],
    "python.defaultInterpreterPath": "${workspaceFolder}/.venv/bin/python"
}